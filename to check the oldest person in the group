def age (Ram,Rahul, Shivani):
    if Ram>=Rahul and Ram>=Shivani:
        print ("Ram is eldest")
    else:
        print ("Ram is not eldest")

#age (15,5,10)


def age1(Ram, Rahul, Shiv):
    if Ram > Rahul:
        if Rahul > Shiv:
            print("Ram is oldest")
    elif Rahul > Ram:
        if Rahul > Shiv:
            print("Rahul is oldest")
    else:
        print("Shiv is oldest")

age1(15, 5, 10)